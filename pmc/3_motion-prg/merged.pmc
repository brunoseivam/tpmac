      DWELL0
      X(M(Q75+0))Y(M(Q77+0))			;goto ${m1}RqsPos & ${m2}RqsPos
    ELSE
      X(M(Q75+0)-M(Q76+0))Y(M(Q77+0))		;goto ${m1}RqsPos-${m1}RqsBkl & ${m2}RqsPos
      DWELL0
      X(M(Q75+0))				;goto ${m1}RqsPos
    ENDIF
  ELSE
    IF( Q3>0 AND Q4<0 OR Q3<0 AND Q4>0)
      X(M(Q75+0))Y(M(Q77+0)-M(Q78+0))		;goto ${m1}RqsPos & ${m2}RqsPos-${m2}RqsBkl
      DWELL0
      Y(M(Q77+0))				;goto ${m2}RqsPos
    ELSE
      X(M(Q75+0))Y(M(Q77+0))			;goto ${m1}RqsPos & ${m2}RqsPos
    ENDIF
  ENDIF
