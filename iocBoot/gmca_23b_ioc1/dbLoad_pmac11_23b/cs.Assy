file $(TPMAC)/csDb/CS_Tsub.db
{
 	pattern { assy, x1, x2, d1, d2, m1, m2, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac11:
  { 23b:CS:Av:, 23b:CS:ZC:, 23b:CS:ZS:, 23b:CS:ZT:, 23b:CS:ZB:, 23b:CS:mz1:, 23b:CS:mz2:, RqsPos, 0, tsubCSAp }
  { 23b:CS:Ah:, 23b:CS:YC:, 23b:CS:YS:, 23b:CS:YO:, 23b:CS:YI:, 23b:CS:my1:, 23b:CS:my2:, RqsPos, 0, tsubCSAp }
  { 23b:CS:Av:, 23b:CS:ZC:, 23b:CS:ZS:, 23b:CS:ZT:, 23b:CS:ZB:, 23b:CS:mz1:, 23b:CS:mz2:, RqsBkl, 1, tsubCSAp }
  { 23b:CS:Ah:, 23b:CS:YC:, 23b:CS:YS:, 23b:CS:YO:, 23b:CS:YI:, 23b:CS:my1:, 23b:CS:my2:, RqsBkl, 1, tsubCSAp }
}
