file $(TPMAC)/xyzDb/XYZ_Calibrate.db
{
	pattern { assy, d1, d2, d3 }
# pmac10:
	{ 23b:MO:Tn2:,  23b:MO:P2:,   23b:MO:R2:,   23b:MO:W2: }
}
#----------------------------------------------------------------------------  
file $(TPMAC)/xyzDb/XYZ_LimAmp.db
{
 	pattern { assy, d1, d2, d3 }
# pmac10:
	{ 23b:MO:Tn2:,  23b:MO:P2:,   23b:MO:R2:,   23b:MO:W2: }
