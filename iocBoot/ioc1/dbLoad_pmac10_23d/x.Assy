file "$(TPMAC)/xDb/X_Calibrate.db"
{
        pattern { assy, d1 }
# pmac10:
        { 23d:MO:Tn1:,  23d:MO:R1:  }
        { 23d:BP:Mo:,   23d:BP:MOZ: }
        { 23d:BP:Kb:,   23d:BP:KBZ: }
}
#---------------------------------------------------------------------------- 
file "$(TPMAC)/xDb/X_LimAmp.db"
{
 	pattern { assy, d1 }
# pmac10:
        { 23d:MO:Tn1:,  23d:MO:R1:  }
        { 23d:BP:Mo:,   23d:BP:MOZ: }
        { 23d:BP:Kb:,   23d:BP:KBZ: }
