#---------------------------------------------------------------------------- 
file "$(TPMAC)/xDb/X_Tsub.db"
{
	pattern { assy, d1, m1, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac10:
        { 23d:MO:Tn1:, 23d:MO:R1:,  23d:MO:mr1:, RqsPos, 0, tsubXPs }
        { 23d:MO:Tn1:, 23d:MO:R1:,  23d:MO:mr1:, RqsBkl, 1, tsubXPs }
        { 23d:BP:Mo:,  23d:BP:MOZ:, 23d:BP:mmo:, RqsPos, 0, tsubXPs }
        { 23d:BP:Mo:,  23d:BP:MOZ:, 23d:BP:mmo:, RqsBkl, 1, tsubXPs }
        { 23d:BP:Kb:,  23d:BP:KBZ:, 23d:BP:mkb:, RqsPos, 0, tsubXPs }
        { 23d:BP:Kb:,  23d:BP:KBZ:, 23d:BP:mkb:, RqsBkl, 1, tsubXPs }
}
#---------------------------------------------------------------------------- 
file "$(TPMAC)/xDb/X_TsubRbk.db"
{
	pattern { assy, d1, desc_d1, egu_d1, m1, val, nla, tsub }
# pmac10:
        { 23d:MO:Tn1:, 23d:MO:R1:,  "Crystal-1 Roll", "(mrad)", 23d:MO:mr1:, ActPos, 0, tsubXPs }
        { 23d:BP:Mo:,  23d:BP:MOZ:, "DCM BMP Z",      "(mm)",   23d:BP:mmo:, ActPos, 0, tsubXPs }
        { 23d:BP:Kb:,  23d:BP:KBZ:, "KBM BPM Z",      "(mm)",   23d:BP:mkb:, ActPos, 0, tsubXPs }
}
#---------------------------------------------------------------------------- 
file "$(TPMAC)/xDb/X_TsubSpeed.db"
{
  pattern { assy, d1, egu_d1, m1, tsub }
# pmac10:
        { 23d:MO:Tn1:, 23d:MO:R1:,  "(mrad/s)", 23d:MO:mr1:, tsubXPs }
        { 23d:BP:Mo:,  23d:BP:MOZ:, "(mm/s)",   23d:BP:mmo:, tsubXPs }
        { 23d:BP:Kb:,  23d:BP:KBZ:, "(mm/s)",   23d:BP:mkb:, tsubXPs }
}
