#----------------------------------------------------------------------------
file "$(TPMAC)/hsDb/HS_Tsub.db"
{
 	pattern { assy, x1, x2, d1, d2, m1, m2, val, nla, tsub }
# tsub's are C programs implementing conversion between drives <-> motors <-> axes.
# nla=0/1 correspond to absolute/relative conversion.
# pmac20:
  { 23i:GS:Av:, 23i:GS:ZC:, 23i:GS:ZS:, 23i:GS:ZT:, 23i:GS:ZB:, 23i:GS:mt:,  23i:GS:mb:,  RqsPos, 0, tsubHSAp }
  { 23i:GS:Av:, 23i:GS:ZC:, 23i:GS:ZS:, 23i:GS:ZT:, 23i:GS:ZB:, 23i:GS:mt:,  23i:GS:mb:,  RqsBkl, 1, tsubHSAp }
  { 23i:GS:Ah:, 23i:GS:YC:, 23i:GS:YS:, 23i:GS:YO:, 23i:GS:YI:, 23i:GS:mo:,  23i:GS:mi:,  RqsPos, 0, tsubHSAp }
  { 23i:GS:Ah:, 23i:GS:YC:, 23i:GS:YS:, 23i:GS:YO:, 23i:GS:YI:, 23i:GS:mo:,  23i:GS:mi:,  RqsBkl, 1, tsubHSAp }
}
